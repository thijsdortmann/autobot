    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "angular-bootstrap": "~0.13.0",
    "angular-material": "~0.10.0",
    "angular-google-maps": "~2.1.5",
    "angular-route": "~1.4.3",
    "angular-resource": "~1.4.3",
    "angular-messages": "~1.4.3",
    "bootstrap": "~3.3.5",
    "angular-ui-router": "~0.2.15",
    "components-font-awesome": "~4.3.0"
  }
